"*":
  "":
    eslintrc: "source.json"
    nvmrc: "source.json"
  "activate-power-mode":
    autoToggle: false
    particles:
      enabled: true
      size:
        max: 5
        min: 1
      spawnCount:
        min: 2
    screenShake:
      maxIntensity: 4
      minIntensity: 2
  alignment: {}
  "atom-beautify":
    general:
      _analyticsUserId: "061edd24-4d2e-4dd9-8a6c-145dcb149a67"
    json:
      brace_style: "expand"
  "atom-easy-jsdoc": {}
  "atom-material-ui":
    colors:
      genAccent: false
      paintCursor: false
    fonts:
      fontSize: 14
    tabs:
      compactTabs: true
      noTabMinWidth: true
    treeView:
      blendTabs: false
      compactList: true
    ui:
      panelContrast: true
  "autoclose-html":
    additionalGrammars: [
      "*"
    ]
    forceInline: [
      "title"
      "h1"
      "h2"
      "h3"
      "h4"
      "h5"
      "h6"
      "pre"
    ]
    makeNeverCloseSelfClosing: false
  autosave:
    enabled: true
  build:
    overrideThemeColors: false
    panelVisibility: "Hidden"
    saveOnBuild: true
    selectTriggers: false
    statusBar: "Disable"
    stealFocus: false
  core:
    customFileTypes:
      "source.git-config": [
        "gitconfig"
      ]
      "source.json": [
        "conf"
      ]
      "source.plist": [
        "plist"
      ]
      "text.html.basic": [
        "pattern"
        "list"
        "item"
        "block"
        "page"
      ]
    disabledPackages: [
      "exception-reporting"
      "metrics"
      "status-bar"
      "npm"
      "build"
      "merge-conflicts"
      "markdown-scroll-sync"
      "linter-write-good"
      "regex-railroad-diagram"
      "markdown-toc"
      "build-gulp"
      "rainbow-selection"
    ]
    ignoredNames: [
      ".git"
      ".svn"
      ".DS_Store"
      ".scssc"
      ".sass-cache"
      "*.map"
      ".cache"
      ".pyc"
      ".sqlite3"
      ".exe"
      ".dll"
      ".sublime-workspace"
      ".sublime-projects"
      "node_modules"
      "bower_components"
      "target"
      ".rej"
      ".codekit-cache"
      ".idea"
      "_archive"
      "_archived"
      "*.jar"
      "*.built.*"
    ]
    openEmptyEditorOnStart: false
    packagesWithKeymapsDisabled: [
      "hey-pane"
    ]
    projectHome: "~/Projects/"
    telemetryConsent: "limited"
    themes: [
      "atom-material-ui"
      "cake-syntax"
    ]
  "custom-title":
    template: "<% if (fileInProject) { %><%= relativeFilePath.replace('src/js/', '') %><% } %><% if (fileInProject && projectPath) { %> - <% } %><% if (projectPath) { %><%= projectName %><% } %>"
  docblockr:
    align_tags: "no"
    lower_case_primitives: true
    newline_after_block: true
    spacer_between_sections: true
  editor:
    autoIndentOnPaste: false
    fontFamily: "Source Code Pro"
    fontSize: 11
    invisibles: {}
    normalizeIndentOnPaste: false
    showIndentGuide: true
    showInvisibles: true
    softTabs: false
    softWrap: false
  "file-types":
    "^\\..*rc$": "text.plain"
  "fuzzy-finder":
    preserveLastSearch: true
  minimap:
    plugins:
      pigments: true
      pigmentsDecorationsZIndex: 0
  "nord-atom-ui":
    tabSizing: false
  "package-sync":
    createOnChange: true
    forceOverwrite: true
  "plantuml-preview":
    jarLocation: "/Users/cshaver/Development/plantuml/plantuml.jar"
    useTempDir: false
  "quick-highlight": {}
  "tree-view":
    alwaysOpenExisting: true
    focusOnReveal: false
    hideIgnoredNames: true
    hideVcsIgnoredFiles: true
    squashDirectoryNames: true
  welcome:
    showOnStartup: false
linter:
  errorPanelHeight: 26
  showErrorPanel: false
"linter-jscs":
  esnext: true
  onlyConfig: true
"linter-jshint":
  disableWhenNoJshintrcFileInPath: true
"linter-json-lint":
  allowComments: true
"linter-sass-lint":
  noConfigDisable: true
"linter-scss-lint":
  disableWhenNoConfigFileInPath: true
"linter-write-good":
  severityLevel: "Info"
"markdown-writer":
  fileExtension: ".markdown, .md"
minimap:
  plugins:
    pigments: true
    pigmentsDecorationsZIndex: 0
    "split-diff": false
    "split-diffDecorationsZIndex": 0
"package-sync":
  forceOverwrite: true
preview:
  _analyticsUserId: "8bef3155-0bbb-46c0-a4e3-3b459eed9c47"
"release-notes":
  viewedVersion: "0.94.0"
"spell-check":
  grammars: [
    "source.gfm"
    "text.git-commit"
    "text.plain"
    "text.plain.null-grammar"
  ]
"split-diff":
  ignoreWhitespace: true
tabs:
  enableVcsColoring: true
  usePreviewTabs: true
"tree-view":
  alwaysOpenExisting: true
  hideIgnoredNames: true
  hideVcsIgnoredFiles: true
  squashDirectoryNames: true
welcome:
  showOnStartup: false
wordcount:
  alwaysOn: true
